{
  "deno.enablePaths": [
    "supabase/functions",
    "infra"
  ],
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "python.defaultInterpreterPath": "${workspaceFolder}/.vscode/mise-tools/python",
  "jupyter.widgetScriptSources": [
    "jsdelivr.com",
    "unpkg.com"
  ],
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "mise.configureExtensionsAutomaticallyIgnoreList": [
    "biomejs.biome",
    "denoland.vscode-deno"
  ]
}